{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES6",
        "allowJs": true,
        "checkJs": true,
        "outDir": "js",
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "forceConsistentCasingInFileNames": true,
        "noEmitOnError": true,
        "sourceMap": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
    },
}